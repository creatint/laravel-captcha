{
    "name": "creatint/captcha",
    "type": "library",
    "description": "Captcha for Laravel5.8+",
    "license": "MIT",
    "authors": [
        {
            "name": "Creatint",
            "email": "creatint@163.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "illuminate/config": "^5.8",
        "illuminate/filesystem": "^5.8",
        "illuminate/support": "^5.8",
        "illuminate/session": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "Creatint\\Captcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creatint\\Captcha\\CaptchaServiceProvider"
            ],
            "aliases": {
                "Captcha": "Creatint\\Captcha\\Facades\\Captcha"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
